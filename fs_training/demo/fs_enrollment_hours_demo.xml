    Enrollment Hours Demo Data
    ==========================
    Updates the auto-created enrollment hours with realistic logged hours.
    -->

    <!-- Update hours for Student 1 (Youssef Hamdi) in Basic Class - 45 hours total -->
    <function model="fs.enrollment.hours" name="write">
        <value eval="[
            ('enrollment_id.student_id.name', '=', 'Youssef Hamdi'),
            ('activity_id.discipline_id.code', '=', 'MAN'),
            ('activity_id.flight_type_id.code', '=', 'DUAL'),
        ]" search="True"/>
        <value eval="{'hours_logged': 25.0}"/>
    </function>
